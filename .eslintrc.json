{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "rules": {
    "no-console": "off",
    "no-underscore-dangle": "off",
    "no-multiple-empty-lines": "off",
    "no-unused-vars": "off",
    "no-trailing-spaces": "off",
    "linebreak-style": "off",
    "comma-dangle": "off",
    "operator-linebreak": "off",
    "object-curly-newline": "off",
    "implicit-arrow-linebreak": "off",
    "camelcase": "off"
  }
}
